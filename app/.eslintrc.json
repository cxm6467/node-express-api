{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "no-plusplus": "off",
        "no-unused-vars": "warn",
        "no-restricted-syntax": "warn"
    }
}
